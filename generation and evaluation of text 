        "def generate_review_positive(text, num):\n",
        "  seed_text_positive = text\n",
        "  next_words_positive = num\n",
        "    \n",
        "  for _ in range(next_words_positive):\n",
        "    token_list_positive = tokenizer_positive.texts_to_sequences([seed_text_positive])[0]\n",
        "    token_list_positive = pad_sequences([token_list_positive], maxlen=max_sequence_len_positive-1, padding='pre')\n",
        "    predicted_positive = np.argmax(model.predict(token_list_positive), axis=-1)\n",
        "    output_word_positive = \"\"\n",
        "    for word, index in tokenizer_positive.word_index.items():\n",
        "      if index == predicted_positive:\n",
        "        output_word_positive = word\n",
        "        break\n",
        "    seed_text_positive += \" \" + output_word_positive\n",
        "  return seed_text_positive"
      ],
      "execution_count": 25,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "SuR0zVWWQ24U"
