    {
      "cell_type": "markdown",
      "metadata": {
        "id": "zPaaM8Z18hJu"
      },
      "source": [
        "Positive reviews:"
      ]
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "apcEXp7WhVBs",
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "outputId": "e1bea41d-dccd-4055-baaf-faf82e45aef3"
      },
      "source": [
        "\n",
        "# Create the corpus using the 'review' column that contains the text\n",
        "corpus_positive = create_corpus(data_positive, 'review')\n",
        "\n",
        "# Tokenize the corpus\n",
        "tokenizer_positive = tokenize_corpus(corpus_positive)\n",
        "\n",
        "total_words_positive = len(tokenizer_positive.word_index) + 1\n",
        "\n",
        "print(tokenizer_positive.word_index)\n",
        "print(total_words_positive)\n",
        "print(corpus_positive)\n"
      ],
      "execution_count": 6,
      "outputs": [
        {
          "output_type": "stream",
