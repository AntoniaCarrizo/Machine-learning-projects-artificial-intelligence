      "source": [
        "sequences_negative = []\n",
        "for line in corpus_negative:\n",
        "\ttoken_list_negative = tokenizer_negative.texts_to_sequences([line])[0]\n",
        "\tfor i in range(1, len(token_list_negative)):\n",
        "\t\tn_gram_sequence = token_list_negative[:i+1]\n",
        "\t\tsequences_negative.append(n_gram_sequence)\n",
        "\n",
        "# Pad sequences for equal input length \n",
        "max_sequence_len_negative = max([len(seq) for seq in sequences_negative])\n",
        "sequences_negative = np.array(pad_sequences(sequences_negative, maxlen=max_sequence_len_negative, padding='pre'))\n",
        "\n",
        "# Split sequences between the \"input\" sequence and \"output\" predicted word\n",
        "input_sequences_negative, labels_negative = sequences_negative[:,:-1], sequences_negative[:,-1]\n",
        "# One-hot encode the labels\n",
        "one_hot_labels_negative = tf.keras.utils.to_categorical(labels_negative, num_classes=total_words_negative)"
      ],
      "execution_count": 9,
